{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "rootDir": "./src",
    "outDir": "./dist"
  },
  "include": ["src/active/*.ts"],
  "exclude": [
    "node_modules",
    "**/*.test.ts",
    "**/*test.tsx",
    "dist",
    "**/src/class_primer/*.ts"
  ]
}
